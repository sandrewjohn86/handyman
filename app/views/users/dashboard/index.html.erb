<h1>Users::Dashboard#index</h1>
<p>Find me in app/views/users/dashboard/index.html.erb</p>

<table >
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Actions</th>
			<th>More Actions</th>
		</tr>
		<tr>
			
			<td><%= @user.fname %></td>
			<td><%= @user.lname %></td>
			<td> <%= link_to 'Browse Projects', users_project_quotes_path(@user.id) %></td>

			<td>
			<% if current_user %>
				<%= link_to "Logout" , destroy_user_session_path, method: :delete %>
			<% else %>
				<%= link_to 'Login' , new_user_session_path %>
			<% end %>
			</td>
				
		</tr>
	</table>
