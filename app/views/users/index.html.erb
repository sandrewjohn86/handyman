<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% if @users.exists? %>
	<table >
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Actions</th>
		</tr>

		<% @users.each do |user| %>
			<tr>
				<td><%= user.fname %></td>
				<td><%= user.lname %></td>
				<td>
				<%= link_to 'Edit', edit_user_registration_path(user.id) %>
				</td>
			</tr>
		<% end %>
	</table>
<% else %>
  <p>There are no categories</p>
<% end %>
